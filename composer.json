{
    "name": "laravel/sail",
    "description": "Docker files for running a basic slim application.",
    "keywords": ["slim", "docker"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Arne Ziegert"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "slim/slim": "*",
        "robmorgan/phinx": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
    },
    "extra": {
    },
    "bin": [
        "sail"
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
